<configuration>
    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSSS} %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="nl.feats" level="debug"/>

    <root level="debug">
        <appender-ref ref="console" />
    </root>
</configuration>